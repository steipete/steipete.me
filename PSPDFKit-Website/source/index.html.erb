<!-- CTA -->
<section class="bg-neutral-darker py-4">
  <div class="container py-1">
    <div class="row align-items-center justify-content-center mb-3">
      <div class="col-lg-4 text-center text-lg-right">
        <h2 class="h4 my-0 mb-3 mb-lg-0">
          Try PSPDFKit in Your App Today
        </h2>
      </div>
      <div class="col-lg-8">
        <form
          id="try-now-form-cta-<%= page_slug %>"
          class="d-flex flex-column flex-md-row"
          action="<%= LICENSE_API_URL %>trial_request"
          method="post"
          data-controller="form-remote"
          data-form-remote-client-id-name="trial_request[gclid]"
          data-form-remote-success-url="/try/thanks"
          data-form-remote-recaptcha-site-key="<%= GOOGLE_RECAPTCHA_SITE_KEY %>"
          data-form-remote-recaptcha-action="trial_form_homepage"
          data-action="submit->form-remote#submit">

          <!-- reCAPTCHA -->
          <input type="hidden"
            data-target="form-remote.recaptchaInput"
            name="trial_request[recaptcha_response]"
            value="">

          <!-- Email -->
          <input
            type="email"
            id="trial_request_email"
            class="form-control form-control-brand mr-3 mb-3 mb-md-0 sh-1 border-0"
            name="trial_request[email]"
            data-target="form-remote.email"
            size="45"
            placeholder="name@company.com"
            required>

          <!-- Submit -->
          <button
            type="submit"
            name="button"
            class="btn btn-brand px-5 py-3 sh-1 text-nowrap"
            data-target="form-remote.submit">
            Download Trial
            <i class="fa fa-arrow-right"></i>
          </button>
        </form>
      </div>
    </div>
    <div class="row">
      <div class="col-12 text-center">
        <p class="small text-muted mb-0">
          The data you submit is treated confidentially and will never be disclosed to third parties.<br>
          We may occasionally send you product news and important announcements.
        </p>
      </div>
    </div>
  </div>
</section>

