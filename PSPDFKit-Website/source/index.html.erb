                          <a href="<%= url %>" class="list-group-item list-group-item-action <%= "list-group-item-action-primary" if title == :download_trial %>">
                            <%= title.to_s.titleize %>
                            <i class="fa fa-arrow-right"></i>
                          </a>
                        <% end %>
                      </div>
                      <a href="<%= data[:url] %>" class="bg-neutral-darker w-100 py-2 px-4 btn-block">
                        <p class="small text-neutral-muted mb-0">
                          PSPDFKit for <%= platform_name(platform) %>
                        </p>
                        <span class="btn btn-link">
                          Product Page
                          <i class="fa fa-arrow-right"></i>
                        </span>
                      </a>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        <% end %>
      </div>
    </div>

  </div>
</section>

