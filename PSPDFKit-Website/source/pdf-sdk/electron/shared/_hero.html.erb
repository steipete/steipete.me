<section id="overview">
  <div class="container">
    <div class="row justify-content-center text-center">
      <div class="col-lg-12 mb-2">
        <header>
          <img src="/images/shared/placeholder.gif" data-src="/images/icons/electron-white.svg" alt="Electron" width="40" class="mb-3 lazyload" loading="lazy">
          <h6 class="text-white op-7 mb-4">
            <%= subtitle %>
          </h6>
          <h1 class="h2 text-white mb-2">
            <%= title %>
          </h1>
        </header>
        <p class="text-white op-7 mb-4">
          <%= body %>
        </p>
        <a href="<%= button_url %>" class="btn btn-lg btn-inverse-brand px-5 my-3 sh-2">
          <%= button_text %>
        </a>
      </div>
      <div class="col-lg-10">
        <img src="/images/shared/placeholder.gif" data-src="/images/pdf-sdk/electron/hero.png" class="img-fluid lazyload" loading="lazy">
      </div>
    </div>
  </div>
</section>
